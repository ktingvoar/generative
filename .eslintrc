{
  "extends": [
    "eslint:recommended",
    "p5js",
    "p5js/dom",
    "p5js/sound"
  ],
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "p5": true,
    "preload": true,
    "setup": true,
    "draw": true,
    "mousePressed": true,
    "mouseMoved": true,
    "mouseDragged": true,
    "mouseReleased": true,
    "mouseClicked": true,
    "doubleClicked": true,
    "windowResized": true,
    "touchStarted": true,
    "touchMoved": true,
    "touchEnded": true
  }
}
